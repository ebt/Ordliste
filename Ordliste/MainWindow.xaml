<Window x:Class="Ordliste.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="lettersTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="128"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="237" Margin="10,47,0,0" VerticalAlignment="Top" Width="143" SelectionChanged="listBox_SelectionChanged"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="271,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox x:Name="lengthTextBox" HorizontalAlignment="Left" Height="23" Margin="146,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="61"/>
        <ListBox x:Name="listBox2" HorizontalAlignment="Left" Height="237" Margin="176,47,0,0" VerticalAlignment="Top" Width="143" SelectionChanged="listBox2_SelectionChanged"/>
        <ListBox x:Name="listBox3" HorizontalAlignment="Left" Height="237" Margin="342,47,0,0" VerticalAlignment="Top" Width="143" SelectionChanged="listBox3_SelectionChanged"/>

    </Grid>
</Window>
